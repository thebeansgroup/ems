- title_bar "Editing article", [{"title"=>"Show", "url"=>@article},{"title"=>"Back", "url"=>articles_path}]

%div#active_admin_content.without_sidebar
  - main_content "Article List" do
    = form_for @article do |f|
      %fieldset{ :class=>"inputs"}
        %ol
          %li
            = f.label :title
            = f.text_field :title
          %li
            = f.submit
